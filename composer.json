{
  "name": "mdobak/autocomplete-widget-bundle",
  "type": "symfony-bundle",
  "description": "Autocomplete widget for Symfony",
  "keywords": ["autocomplete"],
  "license": "MIT",
  "authors": [
    {
      "name": "Micha≈Ç Dobaczewski",
      "email": "mdobak@gmail.com"
    }
  ],
  "require": {
    "php": "^5.4 || ^7.0",
    "symfony/form": "^2.3 || ^3.0",
    "symfony/validator": "^2.3 || ^3.0",
    "symfony/framework-bundle": "^2.3 || ^3.0"
  },
  "require-dev": {
    "symfony/framework-bundle": "^2.3 || ^3.0",
    "symfony/twig-bundle": "^2.3 || ^3.0",
    "symfony/dom-crawler": "^2.3 || ^3.0",
    "symfony/phpunit-bridge": "^3.0",
    "doctrine/doctrine-bundle": "^1.0",
    "doctrine/orm": "^2.3",
    "behat/behat": "~3.0",
    "behat/mink": "~1.5",
    "behat/mink-selenium2-driver": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Mdobak\\AutocompleteWidgetBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mdobak\\AutocompleteWidgetBundle\\Tests\\": "Tests/"
    }
  }
